{
  "manifest_version": 2,
  "name": "NoYoutubeMixes",
  "version": "1.0",

  "description": "Removes YouTube Mixes.",

  "icons": {
    "512": "icons/nym-256.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "js": ["noyoutubemixes.js"]
    }
  ],

  "browser_specific_settings": {
    "gecko": {
      "data_collection_permissions": {
        "required": ["none"]
      }
    }
  }

}
